@model IEnumerable<sang.Models.ThoiKhoaBieu>

@{
    ViewBag.Title = "Index";
}

<h2>Thời khóa biểu</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GiaoVien.TenGiaoVien)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lop.TenLop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MonHoc.TenMonHoc)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GiaoVien.TenGiaoVien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lop.TenLop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MonHoc.TenMonHoc)
            </td>
        </tr>
    }

</table>
